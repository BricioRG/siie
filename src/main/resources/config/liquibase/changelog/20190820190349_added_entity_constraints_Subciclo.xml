<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Subciclo.
    -->
    <changeSet id="20190820190349-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="ciclo_id"
                                 baseTableName="subciclo"
                                 constraintName="fk_subciclo_ciclo_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ciclo"/>

        <addForeignKeyConstraint baseColumnNames="escuela_id"
                                 baseTableName="subciclo"
                                 constraintName="fk_subciclo_escuela_id"
                                 referencedColumnNames="id"
                                 referencedTableName="escuela"/>

        <addForeignKeyConstraint baseColumnNames="tipoperiodo_id"
                                 baseTableName="subciclo"
                                 constraintName="fk_subciclo_tipoperiodo_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tipoperiodo"/>

    </changeSet>
</databaseChangeLog>
