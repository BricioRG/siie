<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Usuario.
    -->
    <changeSet id="20190829184041-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="rol_id"
                                 baseTableName="usuario"
                                 constraintName="fk_usuario_rol_id"
                                 referencedColumnNames="id"
                                 referencedTableName="rol"/>

        <addForeignKeyConstraint baseColumnNames="persona_id"
                                 baseTableName="usuario"
                                 constraintName="fk_usuario_persona_id"
                                 referencedColumnNames="id"
                                 referencedTableName="persona"/>

    </changeSet>
</databaseChangeLog>
